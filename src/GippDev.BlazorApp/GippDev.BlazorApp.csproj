    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Components\SectionHeader.razor.css" />
    <None Remove="Pages\Encrypt.razor.css" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Components\SectionHeader.razor.css" />
